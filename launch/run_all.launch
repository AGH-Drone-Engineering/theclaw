<launch>
  <include file="$(find theclaw)/launch/robot.launch" />

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find turtlebot3_gazebo)/worlds/turtlebot3_stage_4.world"/>
  </include>

  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-urdf -model theclaw -x -0.3 -y 0 -z 0.1 -param robot_description" />

  <node pkg="theclaw" type="theclaw_node.py" name="theclaw" />
  <!-- <include file="$(find theclaw)/launch/lidar.launch" /> -->
  <include file="$(find theclaw)/launch/scan_matcher.launch" />
  <include file="$(find theclaw)/launch/nav.launch" />
  <include file="$(find theclaw)/launch/slam.launch" />
  <include file="$(find theclaw)/launch/rviz.launch" />
  <!-- <include file="$(find theclaw)/launch/joystick.launch" /> -->

  <node pkg="theclaw" type="pose2odom.py" name="pose2odom" />

  <node pkg="robot_localization" type="ukf_localization_node" name="ukf_imu_node">
    <param name="imu0" value="/imu" />
    <param name="imu0_remove_gravitational_acceleration" value="true" />
    <param name="two_d_mode" value="true" />
    <param name="map_frame" value="map" />
    <param name="odom_frame" value="odom" />
    <param name="base_link_frame" value="base_link" />
    <param name="world_frame" value="odom" />
    <rosparam param="imu0_config">
      [false, false, false, false, false, true,
       false, false, false, false, false, true,
       true, true, false]
    </rosparam>

    <param name="odom0" value="/scan_matcher/odom" />
    <rosparam param="odom0_config">
      [true, true, false, false, false, true,
       false, false, false, false, false, false,
       false, false, false]
    </rosparam>
  </node>
</launch>
